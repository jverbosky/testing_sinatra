source 'https://rubygems.org'

# list of gems required by this app
gem 'sinatra'
gem 'minitest'
gem 'rack'
gem 'rack-test'

# To generate Gemfile.lock file, run this from terminal:
#   bundle install
#
# To verify that Gemfile is good, run this from terminal:
#   bundle exec ruby tests/tests_app.rb
#
# If something is wrong in Gemfile, will throw an error